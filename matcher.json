{
    "problemMatcher": [
        {
            "owner": "linter",
            "pattern": [
                {
                    "regexp": "^\\[HOOK - .*.py\\] pre_export\\(\\): (.*):(\\d+):(\\d+): (\S*):? (.*) \\((.*)\\)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }        
    ]
}
