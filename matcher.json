{
    "problemMatcher": [
        {
            "owner": "linter",
            "pattern": [
                {
                    "regexp": "^\\[HOOK\\s-\\s.*.py\\]\\spre_export\\(\\):\\s(.*):(\\d+):(\\d+):\\s(\\S*)\\s(.*)\\s\\((.*)\\)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }        
    ]
}
